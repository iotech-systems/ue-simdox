#!/bin/bash


RED_CODE="31"
GRN_CODE="32"
BLU_CODE="34"
RED="\e[0;${RED_CODE}m"
GRN="\e[0;${GRN_CODE}m"
BLU="\e[0;${BLU_CODE}m"
BLDRED="\e[1;${RED_CODE}m"
BLDGRN="\e[1;${GRN_CODE}m"
BLDBLU="\e[1;${BLU_CODE}m"
ITALICRED="\e[3;${RED}m"
ECLR="\e[0m"

TOKEN_FILE="token-file"

echo ""
echo -e "${BLDGRN} + lib-file: .syslib/data${ECLR}"


